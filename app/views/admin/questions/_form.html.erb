<h1><%= question_header(@question) %></h1>

<%= form_with(model: [:admin, @test, @question]) do |form| %>
  <%= render "shared/errors", resource: @question %>

  <div>
    <%= form.label :body %><br>
    <%= form.text_area :body %>
  </div>

  <div>
    <%= form.submit %>
  </div>
<% end %>
<p> <%= link_to "All questions", admin_test_path(@question.test) %> <p>
